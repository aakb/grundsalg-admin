  {% extends 'AppBundle::base.html.twig' %}


{% block body -%}
  <div class="row heading">
    <div class="col-md-9">
      <h1>{{ entity }}</h1>
    </div>
    <div class="col-md-3 text-right">    <a class="btn btn-default btn-default" href="{{ path('interessent_edit', { 'id': entity.id }) }}">{{ 'common.edit' | trans }}</a>

    </div>

  </div>

  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading">{{ icon('chevron-right') }} {{ 'interessent.labels.singular' | trans }}</div>
        <table class="table table-condensed">
          <tbody>
            <tr>
              <th>{{ 'appbundle.interessent.createdby' | trans }}</th>
                <td>{{ entity.createdby }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.createddate' | trans }}</th>
                <td>{{ entity.createddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.modifiedby' | trans }}</th>
                <td>{{ entity.modifiedby }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.modifieddate' | trans }}</th>
                <td>{{ entity.modifieddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.navn' | trans }}</th>
                <td>{{ entity.navn }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.adresse' | trans }}</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.land' | trans }}</th>
                <td>{{ entity.land }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.telefon' | trans }}</th>
                <td>{{ entity.telefon }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.mobil' | trans }}</th>
                <td>{{ entity.mobil }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.koeberemail' | trans }}</th>
                <td>{{ entity.koeberemail }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.navn1' | trans }}</th>
                <td>{{ entity.navn1 }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.adresse1' | trans }}</th>
                <td>{{ entity.adresse1 }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.land1' | trans }}</th>
                <td>{{ entity.land1 }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.telefon1' | trans }}</th>
                <td>{{ entity.telefon1 }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.mobil1' | trans }}</th>
                <td>{{ entity.mobil1 }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.medkoeberemail' | trans }}</th>
                <td>{{ entity.medkoeberemail }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.interessent.notat' | trans }}</th>
                <td>{{ entity.notat }}</td>
            </tr>          </tbody>
        </table>
      </div>
    </div>
  </div>
{% endblock %}
