  {% extends 'AppBundle::base.html.twig' %}


{% block body -%}
  <div class="row heading">
    <div class="col-md-9">
      <h1>{{ entity }}</h1>
    </div>
    <div class="col-md-3 text-right">    <a class="btn btn-default btn-default" href="{{ path('landinspektoer_edit', { 'id': entity.id }) }}">{{ 'common.edit' | trans }}</a>

    </div>

  </div>

  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading">{{ icon('chevron-right') }} {{ 'landinspektoer.labels.singular' | trans }}</div>
        <table class="table table-condensed">
          <tbody>
            <tr>
              <th>{{ 'appbundle.landinspektoer.adresse' | trans }}</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.email' | trans }}</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.mobil' | trans }}</th>
                <td>{{ entity.mobil }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.navn' | trans }}</th>
                <td>{{ entity.navn }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.notat' | trans }}</th>
                <td>{{ entity.notat }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.telefon' | trans }}</th>
                <td>{{ entity.telefon }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.postnrid' | trans }}</th>
                <td>{{ entity.postnrid }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.createdby' | trans }}</th>
                <td>{{ entity.createdby }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.createddate' | trans }}</th>
                <td>{{ entity.createddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.modifiedby' | trans }}</th>
                <td>{{ entity.modifiedby }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.modifieddate' | trans }}</th>
                <td>{{ entity.modifieddate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
              <th>{{ 'appbundle.landinspektoer.active' | trans }}</th>
                <td>{{ entity.active }}</td>
            </tr>          </tbody>
        </table>
      </div>
    </div>
  </div>
{% endblock %}
