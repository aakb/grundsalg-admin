{% extends 'base.html.twig' %}

{% block body %}
    <h1>Salgsomraade</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ salgsomraade.id }}</td>
            </tr>
            <tr>
                <th>Createdby</th>
                <td>{{ salgsomraade.createdby }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{% if salgsomraade.createddate %}{{ salgsomraade.createddate|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Modifiedby</th>
                <td>{{ salgsomraade.modifiedby }}</td>
            </tr>
            <tr>
                <th>Modifieddate</th>
                <td>{% if salgsomraade.modifieddate %}{{ salgsomraade.modifieddate|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Nr</th>
                <td>{{ salgsomraade.nr }}</td>
            </tr>
            <tr>
                <th>Titel</th>
                <td>{{ salgsomraade.titel }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ salgsomraade.type }}</td>
            </tr>
            <tr>
                <th>Matrikkel1</th>
                <td>{{ salgsomraade.matrikkel1 }}</td>
            </tr>
            <tr>
                <th>Matrikkel2</th>
                <td>{{ salgsomraade.matrikkel2 }}</td>
            </tr>
            <tr>
                <th>Ejerlav</th>
                <td>{{ salgsomraade.ejerlav }}</td>
            </tr>
            <tr>
                <th>Vej</th>
                <td>{{ salgsomraade.vej }}</td>
            </tr>
            <tr>
                <th>Gisurl</th>
                <td>{{ salgsomraade.gisurl }}</td>
            </tr>
            <tr>
                <th>Tilsluttet</th>
                <td>{{ salgsomraade.tilsluttet }}</td>
            </tr>
            <tr>
                <th>Sagsnr</th>
                <td>{{ salgsomraade.sagsnr }}</td>
            </tr>
            <tr>
                <th>Lploebenummer</th>
                <td>{{ salgsomraade.lploebenummer }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_salgsomraade_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_salgsomraade_edit', { 'id': salgsomraade.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
